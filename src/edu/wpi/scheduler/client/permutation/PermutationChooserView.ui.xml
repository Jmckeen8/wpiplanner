<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:edu.wpi.scheduler.client.permutation">
	<ui:style>
		.fullTable {
			width: 100%;
			height: 100%;
		}
	</ui:style>
	<g:SplitLayoutPanel>
		<g:west size="200">
			<g:ScrollPanel>
				<g:VerticalPanel ui:field="courseList" />
			</g:ScrollPanel>
		</g:west>
		<g:west size="170">
			<g:ScrollPanel>
				<g:VerticalPanel ui:field="thumbList" />
			</g:ScrollPanel>
		</g:west>
		<g:center>
			<g:SimplePanel ui:field="body" />
		</g:center> 
	</g:SplitLayoutPanel>
</ui:UiBinder>
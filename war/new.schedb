<course number="2522" name="INTERMEDIATE SPANISH II" course_desc="Cat. I  A continuation of Intermediate Spanish I.  Recommended background: SP 2521.  This course is closed to native speakers of Spanish and heritage speakers except with written permission from the instructor." min-credits="3" max-credits="3" grade-type="normal"><section crn="10278" number="B01" seats="25" availableseats="25" max_waitlist="0" actual_waitlist="0" term="201501" part-of-term="B Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="9:00AM" ends="9:50AM" building="FL" room="311"></period></section><section crn="10279" number="B02" seats="25" availableseats="25" max_waitlist="0" actual_waitlist="0" term="201501" part-of-term="B Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="12:00PM" ends="12:50PM" building="HL" room="154"></period></section><section crn="10280" number="B03" seats="25" availableseats="25" max_waitlist="0" actual_waitlist="0" term="201501" part-of-term="B Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="2:00PM" ends="2:50PM" building="AK" room="232"></period></section><section crn="10281" number="B04" seats="25" availableseats="25" max_waitlist="0" actual_waitlist="0" term="201501" part-of-term="B Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="10:00AM" ends="10:50AM" building="SL" room="407"></period></section><section crn="10571" number="B05" seats="25" availableseats="25" max_waitlist="0" actual_waitlist="0" term="201501" part-of-term="B Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="11:00AM" ends="11:50AM" building="HL" room="114"></period></section></course>
<course number="3521" name="ADVANCED SPANISH I" course_desc="Cat. I  A course that continues to improve students’ language skills while deepening  their understanding of Hispanic cultures. Some of the topics studied are: the  origins of Hispanic cultures in Spain and Spanish America; family; men and  women in Hispanic societies; education; religion.  Recommended background: Intermediate Spanish II. This course is closed to native speakers of Spanish except with written permission  from the instructor." min-credits="3" max-credits="3" grade-type="normal"><section crn="20275" number="C01" seats="25" availableseats="25" max_waitlist="12" actual_waitlist="0" term="201502" part-of-term="C Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="10:00AM" ends="10:50AM" building="HL" room="114"></period></section><section crn="20276" number="C02" seats="25" availableseats="25" max_waitlist="12" actual_waitlist="0" term="201502" part-of-term="C Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="1:00PM" ends="1:50PM" building="FL" room="311"></period></section><section crn="20277" number="C03" seats="25" availableseats="25" max_waitlist="12" actual_waitlist="0" term="201502" part-of-term="C Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="2:00PM" ends="2:50PM" building="FL" room="311"></period></section><section crn="20278" number="C04" seats="25" availableseats="25" max_waitlist="12" actual_waitlist="0" term="201502" part-of-term="C Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="11:00AM" ends="11:50AM" building="HL" room="114"></period></section><section crn="20557" number="C05" seats="25" availableseats="25" max_waitlist="12" actual_waitlist="0" term="201502" part-of-term="C Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mo
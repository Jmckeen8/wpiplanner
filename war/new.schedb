<course number="3522" name="ADVANCED SPANISH II" course_desc="Cat. I  A continuation of Advanced Spanish I.  Recommended background: SP 3521. This course satisfies the Inquiry Practicum requirement.  This course is closed to native speakers of Spanish except with written permission  from the instructor." min-credits="3" max-credits="3" grade-type="normal"><section crn="20593" number="D01" seats="25" availableseats="25" max_waitlist="10" actual_waitlist="0" term="201502" part-of-term="D Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="9:00AM" ends="9:50AM" building="KH" room="115"></period></section><section crn="20279" number="D02" seats="25" availableseats="25" max_waitlist="10" actual_waitlist="0" term="201502" part-of-term="D Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="11:00AM" ends="11:50AM" building="AK" room="233"></period></section><section crn="20280" number="D03" seats="25" availableseats="25" max_waitlist="10" actual_waitlist="0" term="201502" part-of-term="D Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="2:00PM" ends="2:50PM" building="FL" room="311"></period></section><section crn="20281" number="D04" seats="25" availableseats="25" max_waitlist="10" actual_waitlist="0" term="201502" part-of-term="D Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,tue,thu,fri" starts="3:00PM" ends="3:50PM" building="SL" room="104"></period></section><section crn="20560" number="D05" seats="25" availableseats="25" max_waitlist="10" actual_waitlist="0" term="201502" part-of-term="D Term"><period type="Lecture" professor="STAFF" professor_sort_name="STAFF" days="mon,thu" starts="10:00AM" ends="11:50AM" building="AK" room="232"></period></section></course>
